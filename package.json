{
  "name": "agi",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.5",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel --filter=@agi/hisoles",
    "build": "turbo run build --filter=@agi/hisoles",
    "start": "turbo run start --filter=@agi/hisoles",
    "lint": "turbo run lint --filter=@agi/hisoles",
    "lint:fix": "turbo run lint:fix --filter=@agi/hisoles",
    "format": "turbo run format --filter=@agi/hisoles",
    "format:check": "turbo run format:check --filter=@agi/hisoles",
    "typecheck": "turbo run typecheck --filter=@agi/hisoles",
    "check": "turbo run check --filter=@agi/hisoles"
  },
  "devDependencies": {
    "turbo": "2.7.1"
  }
}
